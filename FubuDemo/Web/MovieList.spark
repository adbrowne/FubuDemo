<viewdata model="FubuDemo.Web.MovieListViewModel" />
<content:title>
  Movie List
</content:title>
<h1>Movie List</h1>

<ul>
  <for each="var movie in Model.Movies">
    <li>
      !{this.LinkTo(new FubuDemo.Web.MovieEditInputModel{Id=movie.Id}).Text(movie.Name)}
    </li>
  </for>
</ul>